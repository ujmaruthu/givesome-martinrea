msgid ""
"<p>Dear {{ order.customer }},</p>\n"
"<p>A shipment has been created for your order and here are the details:</p>\n"
"\n"
"{% if shipment.tracking_code %}\n"
"<p>Tracking code: {{ shipment.tracking_code }}</p>\n"
"{% endif %}\n"
"\n"
"<p>Contents:</p>\n"
"<table>\n"
"    <thead>\n"
"        <tr>\n"
"            <th>Product</th>\n"
"            <th>Quantity</th>\n"
"        </tr>\n"
"    </thead>\n"
"    <tbody>\n"
"{% for shipment_product in shipments.total_products %}\n"
"        <tr>\n"
"            <td>{{ shipment_product.product }}</td>\n"
"            <td>{{ shipment_product.quantity }}</td>\n"
"        </tr>\n"
"{% endif %}\n"
"    </tbody>\n"
"</table>\n"
msgstr "<p>Kính gửi {{ order.customer }},</p>\n<p>Một giao hàng đã được tạo ra cho đơn hàng của bạn. Chi tiết như sau:</p>\n{% if shipment.tracking_code %}\n<p>Mã theo dõi: {{ shipment.tracking_code }}</p>\n{% endif %}\n<p>Nội dung:</p>\n<table>\n<thead>\n<tr>\n<th>Sản phẩm</th>\n<th>Số lượng</th>\n</tr>\n</thead>\n<tbody>\n{% for shipment_product in shipments.total_products %}\n<tr>\n<td>{{ shipment_product.product }}</td>\n<td>{{ shipment_product.quantity }}</td>\n</tr>\n{% endif %}\n</tbody>\n</table>\n"

