msgid ""
"<p>Dear {{ order.customer }},</p>\n"
"<p>A shipment has been created for your order and here are the details:</p>\n"
"\n"
"{% if shipment.tracking_code %}\n"
"<p>Tracking code: {{ shipment.tracking_code }}</p>\n"
"{% endif %}\n"
"\n"
"<p>Contents:</p>\n"
"<table>\n"
"    <thead>\n"
"        <tr>\n"
"            <th>Product</th>\n"
"            <th>Quantity</th>\n"
"        </tr>\n"
"    </thead>\n"
"    <tbody>\n"
"{% for shipment_product in shipments.total_products %}\n"
"        <tr>\n"
"            <td>{{ shipment_product.product }}</td>\n"
"            <td>{{ shipment_product.quantity }}</td>\n"
"        </tr>\n"
"{% endif %}\n"
"    </tbody>\n"
"</table>\n"
msgstr "<p>{{ order.customer }},</p>\n<p>Sukurtas nurodymas pristatymui Jūsų užsakymui. Pristatymo nurodymo detalės:</p>\n\n{% if shipment.tracking_code %}\n<p>Sekimo kodas: {{ shipment.tracking_code }}</p>\n{% endif %}\n\n<p>Turinys:</p>\n\n<table>\n\n<thead>\n\n<tr>\n\n<th>Produktas</th>\n\n<th>Kiekis</th>\n\n</tr>\n\n</thead>\n\n<tbody>\n\n{% for shipment_product in shipments.total_products %}\n\n<tr>\n\n<td>{{ shipment_product.product }}</td>\n\n<td>{{ shipment_product.quantity }}</td>\n\n</tr>\n\n{% endif %}\n\n</tbody>\n\n</table>\n"

